generator client {
  provider = "prisma-client-js"
  output = "./generated.clientMongo"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL_MONGO")
}

model cv {
  id                String   @id @default(auto()) @map("_id") @db.ObjectId
  user_id           String
  position          String  
  full_name         String  
  email             String  
  phone_numbers     String?  
  about_your_self   String?  
  skills            String  
  experience        String?  
  languages         String  
  projects          String?  
}